{
  "name": "icons",
  "description": "SVG icons suite for web and user interface design.",
  "version": "0.0.1",
  "author": { "id": "jmjuanes", "name": "Jose Miguel Juanes", "email": "josemijuanes@gmail.com" },
  "directory": "./build",
  "repository": "https://github.com/jviz/module-icons",
  "dependencies": { },
  "build": [ "build" ],
  "install": [ "install" ],
  "tasks":
  {
    "build":
    [
      { "name": "src", "args": [ "./js/coverviewer.js", "./js/**.js" ] },
      { "name": "concat", "args": "coverviewer.js" },
      { "name": "dest", "args": "./" }
    ],
    "install":
    [
      { "name": "src", "args": "./scss/**.scss" },
      { "name": "sass" },
      { "name": "dest", "args": "./" }
    ]
  }
}
